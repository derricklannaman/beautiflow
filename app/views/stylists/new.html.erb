<h1>Sign Up</h1>
<% if @stylist.errors.any? %>
    <ul>
      <% for message in @stylist.errors.full_messages %>
      <li><%= message %></li>
    </ul>
    <% end %>
<% end %>

<div class="form">
<%= form_for @stylist, :html => {:multipart => true} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>

   <div class="form_buttons">
   <%= f.submit "Sign Up", class: "btn btn-success" %>
  </div>

<% end %>
</div><!-- end form -->



















